<toolchains>
    <toolchain>
        <type>jdk</type>
        <provides>
            <id>Java17</id>
            <version>17</version>
        </provides>
        <configuration>
            <jdkHome>${env.JAVA_HOME}</jdkHome>
        </configuration>
    </toolchain>
    <toolchain>
        <type>jdk</type>
        <provides>
            <id>Java7</id>
            <version>1.7</version>
        </provides>
        <configuration>
            <jdkHome>${env.JAVA_HOME_JDK7}</jdkHome>
        </configuration>
    </toolchain>
</toolchains>